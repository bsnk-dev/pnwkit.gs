{
  "name": "pnwkit.gs",
  "version": "1.0.0",
  "description": "An API V3 SDK for PnW with GraphQL Typings compatible with Google App Scripts.",
  "main": "./build/src/index.js",
  "repository": "https://github.com/bsnk-dev/pnwkit",
  "keywords": [
    "politics and war",
    "pnwkit",
    "pnw",
    "api",
    "bann"
  ],
  "scripts": {
